{
  "name": "kudoo",
  "version": "1.0.0",
  "description": "/kudoo Slack command",
  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "yarn run build && node build/index.js",
    "start:dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "knex": "knex",
    "db:seed:run": "yarn run knex --knexfile src/db/knexConfig.ts seed:run",
    "db:migrate:make": "yarn run knex --knexfile src/db/knexConfig.ts migrate:make",
    "db:migrate:latest": "yarn run knex --knexfile src/db/knexConfig.ts migrate:latest",
    "db:migrate:rollback": "yarn run knex --knexfile src/db/knexConfig.ts migrate:rollback"
  },
  "author": "Konstantin Komelin <konstantin.komelin@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@slack/bolt": "^2.5.0",
    "@types/app-root-path": "^1.2.4",
    "app-root-path": "^3.0.0",
    "dotenv": "^8.2.0",
    "knex": "^0.21.14",
    "mysql2": "^2.2.5",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
